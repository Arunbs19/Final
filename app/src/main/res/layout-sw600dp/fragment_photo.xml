<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.afinal.Fragments.photoFragment">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:id="@+id/txtsignature"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="get sign"
            android:textSize="28sp"
            android:layout_margin="30dp"
            android:textStyle="italic"
            android:gravity="center_horizontal"
            android:padding="20dp"
            android:background="@color/color20pink"/>
        <Button
            android:id="@+id/btn_multiplechoice"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="Multiple Choice"
            android:textStyle="bold"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_margin="30dp"
            android:background="@color/color10blue"/>

        <LinearLayout
            android:id="@+id/dynamic_linear"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_margin="30dp"
            android:layout_marginTop="100dp"
            android:orientation="vertical">

        </LinearLayout>

    </LinearLayout>

</FrameLayout>
